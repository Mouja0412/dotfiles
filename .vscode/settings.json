{
    "workbench.colorTheme": "Tron (rainglow)",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "\"Fira Code\", Menlo, Monaco",
    "editor.fontLigatures": true,
    "scm.defaultViewMode": "tree",
    "terminal.integrated.fontFamily": "\"Fira Code\",Menlo,Monaco",
    "workbench.settings.openDefaultKeybindings": true,
    "window.zoomLevel": 0,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "editor.renderWhitespace": "all",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.globalFindClipboard": true,
    "editor.formatOnPaste": true,
    "editor.suggest.shareSuggestSelections": true,
    "workbench.commandPalette.preserveInput": true,
    "workbench.fontAliasing": "auto",
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.settings.useSplitJSON": true,
    "window.titleBarStyle": "native",
    "window.newWindowDimensions": "inherit",
    "explorer.incrementalNaming": "smart",
    "search.maintainFileSearchCache": true,
    "search.quickOpen.includeSymbols": true,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "debug.console.fontFamily": "\"Fira Code\", Monaco, \"M",
    "debug.inlineValues": true,
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "vsicons.dontShowNewVersionMessage": true,
    "slack.username": "",
    "html.format.wrapLineLength": 80,
    "kite.showWelcomeNotificationOnStartup": false,
    "better-comments.highlightPlainText": true,
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "markdown.preview.fontFamily": "'Fira Sans', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "markdown.trace": "verbose",
    "files.trimTrailingWhitespace": true,
    "vim.camelCaseMotion.enable": true,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.visual": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.enableNeovim": false,
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.replaceWithRegister": true,
    "vim.sneak": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "rewrap.reformat": true,
    "rewrap.wrappingColumn": 80,
    "cSpell.userWords": [
        "Jupyter",
        "autobot",
        "conda",
        "imgkit",
        "ionlights",
        "itertools",
        "joblib",
        "jupyterlab",
        "kaggle",
        "matplotlib",
        "mtom",
        "nargs",
        "nbconvert",
        "nbformat",
        "networkx",
        "numpy",
        "plotly",
        "scikit",
        "seaborn",
        "tqdm",
        "ucfai's"
    ],
    "workbench.startupEditor": "newUntitledFile",
    "python.dataScience.exportWithOutputEnabled": true,
    "python.condaPath": "/usr/local/Caskroom/miniconda/base/condabin/conda",
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "python.analysis.memory.keepLibraryAst": true,
    "python.autoComplete.addBrackets": true,
    "python.dataScience.askForKernelRestart": false,
    "python.dataScience.magicCommandsAsComments": true,
    "python.diagnostics.sourceMapsEnabled": false,
    "vsicons.presets.foldersAllDefaultIcon": true,
    "vsicons.projectDetection.autoReload": true,
    "python.dataScience.ignoreVscodeTheme": true,
    "files.trimFinalNewlines": true,
    "files.autoGuessEncoding": true,
    "python.dataScience.collapseCellInputCodeByDefault": false,
    "python.dataScience.maxOutputSize": -1,
    "editor.formatOnType": true,
    "workbench.editor.labelFormat": "medium",
    "editor.rulers": [
        88,
        110,
        132
    ],
    "files.associations": {
        "*.yml": "yaml"
    },
    "python.formatting.provider": "black",
    "terminal.integrated.experimentalRefreshOnResume": true,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "terminal.integrated.inheritEnv": false,
    "editor.lineNumbers": "relative",
    "explorer.confirmDragAndDrop": false,
